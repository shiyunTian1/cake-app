t-inner\">\r\n\t\t\t\t\t\t<form>\r\n                            用户名：<input type=\"text\" value=\"\" placeholder=\"请输入用户名\" v-model=\"name\" @blur.prevent=\"checkName()\"/></input>\r\n                            密码：<input type=\"password\" value=\"\" placeholder=\"请输入密码\" v-model=\"pwd\"/></input>\r\n                            确认密码：<input type=\"password\" value=\"\" placeholder=\"请输入密码\"  v-model=\"pwd2\"/></input>\r\n                            <mt-button size=\"large\" @click=\"handleReg()\" type=\"primary\">注册</mt-button>\r\n                        </form>\r\n\t\t\t\t\t</div>\r\n\t\t\t\t</div>\r\n\t\t\t\t<div class=\"mui-card-footer\">页脚</div>\r\n\t\t\t</div>  \r\n        </div>\r\n</template>\r\n<script>\r\nimport {Toast} from 'mint-ui'\r\nexport default {\r\n      data() {\r\n         return {\r\n             //双向绑定的数值\r\n             name:\"\",\r\n             pwd:\"\",\r\n             pwd2:\"\",\r\n             isSumit:false//是否允许提交\r\n         }\r\n     },\r\n    methods: {\r\n        checkName(){\r\n            //获取用户输入的用户名\r\n            var n=this.name;\r\n            //发送AJAX请求\r\n            var url=\"http://127.0.0.1:3000/existsName?name=\"+n;\r\n            //提示消息\r\n            this.axios.get(url).then(result=>{\r\n               // console.log(result.data.msg)\r\n               if(result.data.code>0){\r\n                Toast(\"用户名可以使用\")\r\n                   this.isSumit=true;\r\n               }else{\r\n                Toast(\"用户名已存在请修改\")\r\n                   this.isSumit=false;\r\n               }\r\n            })\r\n        },\r\n        handleReg(){\r\n            if(!this.isSumit){\r\n                Toast(\"用户名已存在请修改\")\r\n                //禁止提交\r\n                return;\r\n            }\r\n            //console.log(123);\r\n            //获取用户输入用户名 密码确认密码 为了让变量名短一点\r\n            var n=this.name;\r\n            var p=this.pwd;\r\n            var p2=this.pwd2;\r\n            //  console.log(n+\"|\"+p+\"|\"+cp)\r\n            //验证\r\n            var regname=/^[0-9a-z_]{8,12}$/i;\r\n            var regpwd=/^[0-9a-z]{8,12}$/i;\r\n            if(!regname.test(n)){\r\n                Toast(\"用户名格式不正确\")\r\n                return;\r\n            }\r\n            if(!regpwd.test(p)){\r\n                Toast(\"密码格式不正确\")\r\n                return;\r\n            }\r\n            if(p!=p2){\r\n                Toast(\"两次密码不一致，请重新输入\")\r\n            }\r\n            //发送AJAX请求\r\n            var url=\"http://127.0.0.1:3000/register?name=\"+n+\"&pwd=\"+p;\r\n            this.axios.get(url).then(result=>{\r\n                //console.log(result.data);\r\n            //提示结果\r\n              Toast(result.data.msg);\r\n            })\r\n        }\r\n    },\r\n   \r\n}\r\n</script>\r\n<style>\r\n    \r\n</style>\r\n\r\n"],"sourceRoot":"src\\components\\tabbar"}]}