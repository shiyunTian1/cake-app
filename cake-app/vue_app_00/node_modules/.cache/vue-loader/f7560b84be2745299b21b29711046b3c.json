disabled=\"Disabled\" type=\"primary\" @click=\"getMore\">加载更多</mt-button>\r\n  </div>\r\n</template>\r\n<script>\r\n  export default {\r\n    data(){\r\n      return{\r\n        list:[],   //返回数据\r\n        pageIndex:0,   //当前页码\r\n        pageSize:7,   //页大小\r\n        pageCount:1,   //总页数\r\n        Disabled:false\r\n      }\r\n    },\r\n    methods:{\r\n      getMore(){\r\n        this.pageIndex++;\r\n        this.axios.get(\"http://127.0.0.1:3000/getNews\",\r\n          {params:{pno:this.pageIndex,pageSize:this.pageSize}        \r\n          }\r\n        ).then(result=>{\r\n          this.list=this.list.concat(result.data.data);\r\n          this.pageCount=result.data.pageCount;\r\n          if(this.pageCount===this.pageIndex || this.list.length<this.pageSize)\r\n          this.Disabled=true;          \r\n        })\r\n      }\r\n    },\r\n    created() {\r\n      this.getMore();\r\n    },\r\n  }\r\n</script>\r\n<style scoped>\r\n.app-news .mui-table-view-cell{ margin-right:15px; }\r\n.app-news .mui-media-body{ color:#333; font-size:14px;}\r\n.app-news .mui-ellipsis{ color:#26a2ff; display:flex; justify-content:space-between; }\r\n</style>\r\n\r\n\r\n"]}]}